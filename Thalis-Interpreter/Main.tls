Import IO;

class Vec2
{
	public void Print()
	{
		IO.Print("(");
		IO.Print(x);
		IO.Print(", ");
		IO.Print(y);
		IO.Println(")");
	}

	public void Set(real32 x, real32 y)
	{
		this.x = x;
		this.y = y;
	}

	real32 x;
	real32 y;
};

class Main
{
	public static void Main()
	{
		uint32 counter = 0;
		while(counter < 100)
		{
			for(uint32 i = 0; i < counter; i++)
			{
				IO.Println(i);
			}

			counter++;
		}
	}
};