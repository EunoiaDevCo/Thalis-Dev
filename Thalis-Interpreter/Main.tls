Import IO;

class Vec2
{
	public void Print()
	{
		IO.Print("(");
		IO.Print(x);
		IO.Print(", ");
		IO.Print(y);
		IO.Println(")");
	}

	public void Set(real32 ax, real32 ay)
	{
		x = ax;
		y = ay;
	}

	real32 x;
	real32 y;
};

