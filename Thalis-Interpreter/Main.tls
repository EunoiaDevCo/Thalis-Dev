Import IO;

class Vec2
{
	public Vec2(real32 x, real32 y)
	{
		this.x = x;
		this.y = y;
	}

	public ~Vec2()
	{
		IO.Println("Called ~Vec2()");
	}

	public void Print()
	{
		IO.Print("(");
		IO.Print(x);
		IO.Print(", ");
		IO.Print(y);
		IO.Println(")");
	}

	public void Set(real32 x, real32 y)
	{
		this.x = x;
		this.y = y;
	}

	real32 x;
	real32 y;
};

class Main
{
	public static void Main()
	{
		Vec2* vec = new Vec2(10.0, 10.0);
		vec.Print();
		delete vec;
	}
};